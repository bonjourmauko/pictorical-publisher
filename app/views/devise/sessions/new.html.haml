#login-canvas
  #notice
    = notice


  = form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f|
    .input
      .label
      .field-block
        = f.text_field :email
        %br
        &nbsp;Email
      .field-block
        = f.text_field :password
        %br
        &nbsp;Password
      .field-block
        = f.submit "Sign in"
        %br
        &nbsp;

    - if devise_mapping.rememberable?
      %p
        = f.check_box :remember_me
        = f.label :remember_me
  = #render :partial => "devise/shared/links"
